<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Super Market</title>

    <link href="~/Content/Style.css" rel="stylesheet" />

    <link href="~/assert/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

    <link href="~/assert/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

    <link href="~/assert/vendors/nprogress/nprogress.css" rel="stylesheet" />

    <link href="~/assert/vendors/iCheck/skins/flat/green.css" rel="stylesheet" />

    <link href="~/assert/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" />

    <link href="~/assert/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet" />

    <link href="~/assert/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet" />

    <link href="~/assert/build/css/custom.min.css" rel="stylesheet" />

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

    <link href="https://cdn.jsdelivr.net/npm/select2@4.0.12/dist/css/select2.min.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.8/js/select2.min.js" defer></script>

    <link href="https://cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css" rel="stylesheet" />
    <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js" defer></script>

    <link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">
    <script src="https://gitcdn.github.io/bootstrap-toggle/2.2.2/js/bootstrap-toggle.min.js"></script>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery.inputmask/3.3.4/css/inputmask.min.css" />

    <script>
        $(document).ready(function () {
            $('table.display').DataTable();
        });
    </script>

    <script>
        $(document).ready(function () {
            $('.js-example-basic-single').select2();
        });
    </script>
</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <center><a href="@Url.Action("Index", "Home")" class="site_title"><span>Super Market</span></a></center>
                    </div>
                    <div class="clearfix"></div>

                    <br />

                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            @Html.Partial("_MainMenu")
                        </div>
                    </div>
                </div>
            </div>

            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    Account
                                    <span class=" fa fa-angle-down"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-usermenu pull-right">
                                    <li><a href="../User/Logout"><i class="fa fa-sign-out pull-right"></i> Logout</a></li>
                                </ul>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <div class="right_col" role="main">
                @RenderBody()
            </div>
            <footer>
                <div class="pull-right">
                    &copy; @DateTime.Now.Year - Super Market
                </div>
                <div class="clearfix"></div>
            </footer>

        </div>
    </div>

    <script src="~/assert/vendors/jquery/dist/jquery.min.js"></script>

    <script src="~/assert/vendors/bootstrap/dist/js/bootstrap.min.js"></script>

    <script src="~/assert/vendors/fastclick/lib/fastclick.js"></script>

    <script src="~/assert/vendors/nprogress/nprogress.js"></script>

    <script src="~/assert/vendors/Chart.js/dist/Chart.min.js"></script>

    <script src="~/assert/vendors/gauge.js/dist/gauge.min.js"></script>

    <script src="~/assert/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>

    <script src="~/assert/vendors/iCheck/icheck.min.js"></script>

    <script src="~/assert/vendors/skycons/skycons.js"></script>

    <script src="~/assert/vendors/Flot/jquery.flot.js"></script>
    <script src="~/assert/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="~/assert/vendors/Flot/jquery.flot.time.js"></script>
    <script src="~/assert/vendors/Flot/jquery.flot.stack.js"></script>
    <script src="~/assert/vendors/Flot/jquery.flot.resize.js"></script>

    <script src="~/assert/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
    <script src="~/assert/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="~/assert/vendors/flot.curvedlines/curvedLines.js"></script>

    <script src="~/assert/vendors/DateJS/build/date.js"></script>

    <script src="~/assert/vendors/jqvmap/dist/jquery.vmap.js"></script>
    <script src="~/assert/vendors/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <script src="~/assert/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>

    <script src="~/assert/vendors/moment/min/moment.min.js"></script>
    <script src="~/assert/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>

    <script src="~/assert/build/js/custom.min.js"></script>
    <script src="~/assert/vendors/jquery.inputmask/dist/min/jquery.inputmask.bundle.min.js"></script>

    <script type="51ef1a50149277e0d75270eb-text/javascript">
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-23581568-13', 'auto');
        ga('send', 'pageview');

    </script>
    @RenderSection("scripts", required: false)
</body>
</html>